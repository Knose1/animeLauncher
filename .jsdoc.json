{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc"]
	},
	"source": {
		"include": ["main.js","./src/"],
		"includePattern": ".js$",
		"excludePattern": "(node_modules/|docs)"
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": true,
		"useLongnameInNav": false,
		"systemName": "Test",
		"systemSummary": "JSDoc",
		"systemLogo": "./public/asset/ico/ico_64.png",
		"stylesheets": ["../_jsdocCustomCss/index.css"]
	},
	"opts": {
		"destination": "./docs/",
		"encoding": "utf8",
		"private": true,
		"recurse": true,
		"template": "./node_modules/foodoc/template"
	}
}