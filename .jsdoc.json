{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc"]
	},
	"source": {
		"include": ["main.js","./src/"],
		"includePattern": ".js$",
		"excludePattern": "(node_modules/|docs)"
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": true,
		"useLongnameInNav": false,
		"systemName": "Test",
		"systemSummary": "JSDoc",
		"systemLogo": "./public/asset/ico/ico_64.png",
		"favicon": "./public/asset/ico/ico_64.png",
		"systemColor": "hsl(0, 56%, 46%)",
		"stylesheets": ["./_jsdocCustomScripts/index.css"],
		"scripts": ["./_jsdocCustomScripts/jsColorUtils.js", "./_jsdocCustomScripts/index.js"]
	},
	"opts": {
		"destination": "./docs/",
		"encoding": "utf8",
		"private": true,
		"recurse": true,
		"template": "./node_modules/foodoc/template"
	}
}